# .env.example
# Copy this file to .env and fill in your values

# AIF Core Service Configuration
# The base URL of the AIF Core Service (Heimdall)
AIF_SDK_CORE_URL=http://localhost:5000

# Your Agent Builder API Key
# This is required for authenticating with the AIF Core Service
AIF_SDK_ISSUER_API_KEY=your-agent-builder-api-key-here

# Optional: Test environment configuration
# These are used when running tests
AIF_SDK_CORE_URL_TEST=http://localhost:5000
AIF_SDK_ISSUER_API_KEY_TEST=test-api-key-for-testing

# Optional: Logging configuration
# Set to DEBUG for more verbose output
LOG_LEVEL=INFO